# Coordinator env template (DO NOT COMMIT SECRETS)
#
# Create your local env file:
#   cp env.template .env
#
# Then run:
#   npm run dev

NETWORKS_SUPPORTED=devnet,testnet,mainnet,localnet

# Server
HOST=0.0.0.0
PORT=3000
CORS_ORIGIN=*

# Intent parsing
MAX_TEXT_LEN=500
DEFAULT_EXPIRY_MINUTES=15
DEFAULT_MAX_SLIPPAGE_BPS=100
PARSER_TIMEOUT_MS=10000
PARSER_IDEMPOTENCY_TTL_MS=600000
PARSER_RATE_LIMIT_PER_MINUTE=30

# OpenAI (required for /api/intent/free-text and integration script)
OPENAI_MODEL=gpt-5-nano-2025-08-07
OPENAI_API_KEY=YOUR_KEY_HERE


